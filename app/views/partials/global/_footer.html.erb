<footer class="footer">
  <div class="footer__container">
    <div class="list--footer-logos">
      <ul class="list__ul">
        <% main_logo.each do |logo| %>
          <li class="list__li">
            <%= image_tag logo[:src], alt: logo[:alt], class: 'list__img' %>
          </li>
        <% end %>
      </ul>
    </div>
    <div>
      <nav class="nav--footer">
        <ul class="nav__ul">
          <% links.each do |link| %>
            <li class="nav__li">
              <%= link_to link[:title], link[:url], class: 'nav__a' %>
            </li>
          <% end %>
        </ul>
      </nav>
      <a class="nav__contact-us"><%= email %></a>
    </div>

    <div class="list--footer-logos">
      <ul class="list__ul">
        <% logos.each do |logo| %>
          <li class="list__li">
            <%= image_tag logo[:src], alt: logo[:alt], class: 'list__img' %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</footer>
