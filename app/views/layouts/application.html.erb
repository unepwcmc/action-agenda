<!DOCTYPE html>
<html>
  <head>
    <title>ActionAgenda</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render partial: '/partials/topbar/topbar-primary' %>
    <%= render partial: '/partials/topbar/topbar-secondary', locals: { links: header_nav } %>
    
    <%= yield %>
    
    <%= render partial:'partials/global/footer', locals: { 
      links: footer_nav, 
      logos: site_logos 
    } %>
  </body>
</html>
